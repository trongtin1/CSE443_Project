@model CSE443_Project.Models.CV

<h2>Chi tiết CV</h2>

<table class="table">
    <tr>
        <th>Người dùng</th>
        <td>@Model.JobSeeker.User.Username (@Model.JobSeeker.User.Email)</td>
    </tr>
    <tr>
        <th>Tiêu đề</th>
        <td>@Model.Title</td>
    </tr>
    <tr>
        <th>Mô tả</th>
        <td>@Model.Description</td>
    </tr>
    <tr>
        <th>Ngày tạo</th>
        <td>@Model.CreatedAt.ToString("dd/MM/yyyy")</td>
    </tr>
    <tr>
        <th>Ngày cập nhật</th>
        <td>@(Model.UpdatedAt.HasValue ? Model.UpdatedAt.Value.ToString("dd/MM/yyyy") : "")</td>
    </tr>
</table>

<a class="btn btn-secondary" href="/Admin/CVs">Quay lại</a>
